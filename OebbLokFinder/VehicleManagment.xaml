<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:OebbLokFinder"
             xmlns:converter="clr-namespace:OebbLokFinder.Converter"
             x:DataType="local:VehicleManagment"
             x:Class="OebbLokFinder.VehicleManagment"
             Title="Vehicle Managment"
             BackgroundColor="White" x:Name="OebbLokFinder">
    <Grid x:Name="grid" VerticalOptions="Fill">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="AUTO"/>
        </Grid.RowDefinitions>
        <ScrollView Grid.Row="0" VerticalOptions="Start">
            <StackLayout x:Name="VSLVehicles" VerticalOptions="Fill" IsVisible="{Binding Test}">
            </StackLayout>
        </ScrollView>
        <StackLayout Grid.Row="1" x:Name="SLUpdate"  >

        </StackLayout>
    </Grid>
</ContentPage>